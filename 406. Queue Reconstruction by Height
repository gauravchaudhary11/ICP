class Solution {
    public int[][] reconstructQueue(int[][] people) {
        Arrays.sort(people,(a,b)->{
            if(a[0]==b[0]){
                return a[1]-b[1];
            }
            else{
                return b[0]-a[0];
            }
        });
        List<int[]>ans=new ArrayList<>();
        for(int[]person:people){
            ans.add(person[1],person);
        }
        return ans.toArray(new int[people.length][]);
        
    }
}
