class Solution {
    public int sol(int arr[][],int cr,int cc,int er,int ec,int dp[][]){
        if(cr==er && cc==ec){
            return 1;
        }
        if(cr>er || cc >ec){
            return 0;
        }
        if(dp[cr][cc]!=0){
            return dp[cr][cc];
        }
        int v=sol(arr,cr+1,cc,er,ec,dp);
        int h=sol(arr,cr,cc+1,er,ec,dp);
        dp[cr][cc]=h+v;
        return dp[cr][cc];

    }
    public int uniquePaths(int m, int n) {
        int arr[][]=new int[m][n];
        int count=0;
        int dp[][]=new int[m][n];
        return sol(arr,0,0,arr.length-1,arr[0].length-1,dp);
        
    }
}
