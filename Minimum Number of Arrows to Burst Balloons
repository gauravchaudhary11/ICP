class Solution {
    public int findMinArrowShots(int[][] arr) {
        int n = arr.length;
        Arrays.sort(arr,(a,b)->Integer.compare(a[1],b[1]));
        int arrows=1;
        int last = arr[0][1];
        for(int i=1;i<n;i++){
            if(arr[i][0]<=last){
                continue;
            }
            else{
                arrows++;
                last = arr[i][1];
            }
        }
        return arrows;
    }
}
